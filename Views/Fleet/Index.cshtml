@model IEnumerable<FleetManagement.Models.Fleet>

@{
    ViewBag.Title = "Fleets";
}

<h2>Fleets</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FleetID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FleetName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FleetID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FleetName)
        </td>
        
    </tr>
}

</table>
